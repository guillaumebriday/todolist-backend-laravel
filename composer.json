{
    "name": "Todolist Backend",
    "description": "Todolist Backend",
    "keywords": ["todolist", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "4.1.0",
        "laravel/framework": "5.8.29",
        "laravel/slack-notification-channel": "2.0.0",
        "laravel/tinker": "1.0.8",
        "pusher/pusher-php-server": "3.4.1",
        "spatie/laravel-backup": "6.3.1",
        "spatie/laravel-cors": "1.5.1",
        "spatie/laravel-tail": "3.2.4",
        "tymon/jwt-auth": "1.0.0-rc.4.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "1.2.2",
        "filp/whoops": "2.4.1",
        "nunomaduro/collision": "2.1.1",
        "friendsofphp/php-cs-fixer": "2.15.1",
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.2",
        "phpunit/phpunit": "7.5.14"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
